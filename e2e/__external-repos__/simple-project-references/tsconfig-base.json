{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "strict": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "outDir": "./target/",
    "rootDir": "./src/",
    "types": [],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/nested/*",
        "src/nested/src/*"
      ],
      "~": [
        "src/math/divide.ts"
      ],
      "~/*": [
        "src/math/*"
      ]
    }
  }
}
